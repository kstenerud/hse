<?xml version="1.0" encoding="UTF-8"?>
<stylesheet>
	<style name="Title" type="nextapp.echo2.app.Label">
		<properties>
			<property name="font">
				<font bold="true" size="24px" typeface="verdana, arial, sans-serif"/>
			</property>
			<property name="foreground" value="#ffffff"/>
			<property name="layoutData">
				<layout-data type="nextapp.echo2.app.layout.ColumnLayoutData">
					<properties>
						<property name="insets" value="60px 8px 0px 0px"/>
					</properties>
				</layout-data>
			</property>
		</properties>
	</style>
	<style name="MenuBar" type="nextapp.echo2.app.Column">
		<properties>
			<property name="background" value="#eeeeee"/>
		</properties>
	</style>
	<style name="MenuBar" type="nextapp.echo2.app.ContentPane">
		<properties>
			<property name="background" value="#eeeeee"/>
		</properties>
	</style>
	<style name="MenuBar" type="echopointng.ButtonEx">
		<properties>
			<property name="background" value="#eeeeee"/>
			<property name="foreground" value="#000000"/>
			<property name="rolloverBackground" value="#316ac5"/>
			<property name="height" value="24px"/>
			<property name="insets" value="0px 0px"/>
			<property name="outsets" value="0px"/>
			<property name="border">
				<border color="#eeeeee" size="0px" style="none"/>
			</property>
			<property name="rolloverBorder">
				<border color="#eeeeee" size="0px" style="none"/>
			</property>
			<property name="pressedBorder">
				<border color="#eeeeee" size="0px" style="none"/>
			</property>
			<property name="disabledBorder">
				<border color="#eeeeee" size="0px" style="none"/>
			</property>
			<property name="alignment">
				<alignment horizontal="left"/>
			</property>
			<property name="textPosition">
				<alignment horizontal="center"/>
			</property>
		</properties>
	</style>
	<style name="Menu" type="echopointng.ButtonEx">
		<properties>
			<property name="background" value="#ffffff"/>
			<property name="foreground" value="#000000"/>
			<property name="rolloverBackground" value="#316ac5"/>
			<property name="height" value="24px"/>
			<property name="insets" value="0px 0px"/>
			<property name="outsets" value="0px"/>
			<property name="border">
				<border color="#ffffff" size="0px" style="none"/>
			</property>
			<property name="rolloverBorder">
				<border color="#ffffff" size="0px" style="none"/>
			</property>
			<property name="pressedBorder">
				<border color="#ffffff" size="0px" style="none"/>
			</property>
			<property name="disabledBorder">
				<border color="#ffffff" size="0px" style="none"/>
			</property>
			<property name="alignment">
				<alignment horizontal="left"/>
			</property>
			<property name="textPosition">
				<alignment horizontal="center"/>
			</property>
		</properties>
	</style>
	<style name="Requestor" type="nextapp.echo2.app.WindowPane">
		<properties>
			<!-- Container-specific property to enable Internet Explorer PNG Alpha Renderer for shadow window border. -->
			<property name="nextapp.echo2.webcontainer.syncpeer.WindowPanePeer.ieAlphaRenderBorder" type="java.lang.Boolean" value="true"/>
			<property name="titleInsets" value="10px 5px"/>
			<property name="titleBackground" value="#2f2f4f"/>
			<property name="titleForeground" value="#ffffff"/>
			<property name="border">
				<fill-image-border border-insets="17px 17px 23px 23px" content-insets="8px 8px 14px 14px">
					<border-part position="top-left">
						<fill-image>
							<image type="nextapp.echo2.app.ResourceImageReference">
								<resource-image-reference resource="hse/theme/RedmondX/WindowTopLeft.png"/>
							</image>
						</fill-image>
					</border-part>
					<border-part position="top">
						<fill-image>
							<image type="nextapp.echo2.app.ResourceImageReference">
								<resource-image-reference resource="hse/theme/RedmondX/WindowTop.png"/>
							</image>
						</fill-image>
					</border-part>
					<border-part position="top-right">
						<fill-image>
							<image type="nextapp.echo2.app.ResourceImageReference">
								<resource-image-reference resource="hse/theme/RedmondX/WindowTopRight.png"/>
							</image>
						</fill-image>
					</border-part>
					<border-part position="left">
						<fill-image>
							<image type="nextapp.echo2.app.ResourceImageReference">
								<resource-image-reference resource="hse/theme/RedmondX/WindowLeft.png"/>
							</image>
						</fill-image>
					</border-part>
					<border-part position="right">
						<fill-image>
							<image type="nextapp.echo2.app.ResourceImageReference">
								<resource-image-reference resource="hse/theme/RedmondX/WindowRight.png"/>
							</image>
						</fill-image>
					</border-part>
					<border-part position="bottom-left">
						<fill-image>
							<image type="nextapp.echo2.app.ResourceImageReference">
								<resource-image-reference resource="hse/theme/RedmondX/WindowBottomLeft.png"/>
							</image>
						</fill-image>
					</border-part>
					<border-part position="bottom">
						<fill-image>
							<image type="nextapp.echo2.app.ResourceImageReference">
								<resource-image-reference resource="hse/theme/RedmondX/WindowBottom.png"/>
							</image>
						</fill-image>
					</border-part>
					<border-part position="bottom-right">
						<fill-image>
							<image type="nextapp.echo2.app.ResourceImageReference">
								<resource-image-reference resource="hse/theme/RedmondX/WindowBottomRight.png"/>
							</image>
						</fill-image>
					</border-part>
				</fill-image-border>
			</property>
		</properties>
	</style>
	<style name="Requestor" type="nextapp.echo2.app.Row">
		<properties>
			<property name="insets" value="10px 2px"/>
			<property name="cellSpacing" value="3px"/>
			<property name="layoutData">
				<layout-data type="nextapp.echo2.app.layout.SplitPaneLayoutData">
					<properties>
						<property name="overflow" value="OVERFLOW_HIDDEN"/>
						<property name="background" value="#cfcfdf"/>
						<property name="insets" value="2px"/>
					</properties>
				</layout-data>
			</property>
		</properties>
	</style>
	<style name="Generic.Table" type="nextapp.echo2.app.Table">
		<properties>
			<property name="border">
				<border color="#dddddd" size="2px" style="outset"/>
			</property>
		</properties>
	</style>
	<style name="Generic.MajorField" type="nextapp.echo2.app.Label">
		<properties>
			<property name="font">
				<font bold="true" size="10pt" typeface="sans-serif"/>
			</property>
			<property name="layoutData">
				<layout-data type="nextapp.echo2.app.layout.TableLayoutData">
					<properties>
						<property name="insets" value="4px 1px"/>
						<property name="background" value="#d3e5fa"/>
					</properties>
				</layout-data>
			</property>
			<property name="background" value="#d3e5fa"/>
		</properties>
	</style>
	<style name="Generic.MinorField" type="nextapp.echo2.app.Label">
		<properties>
			<property name="layoutData">
				<layout-data type="nextapp.echo2.app.layout.TableLayoutData">
					<properties>
						<property name="insets" value="16px 1px 1px 4px"/>
					</properties>
				</layout-data>
			</property>
		</properties>
	</style>
	<style name="Generic.EditField" type="nextapp.echo2.app.TextField">
		<properties>
			<property name="layoutData">
				<layout-data type="nextapp.echo2.app.layout.TableLayoutData">
					<properties>
						<property name="insets" value="1px 1px"/>
						<property name="background" value="#ece9d8"/>
					</properties>
				</layout-data>
			</property>
			<property name="background" value="#f6f6f2"/>
		</properties>
	</style>
	<style base-name="Generic.MajorField" name="Generic.Header" type="nextapp.echo2.app.Label"></style>
	<style base-name="Generic.MinorField" name="Generic.Value" type="nextapp.echo2.app.Label"></style>
	<style name="TitleBackground" type="nextapp.echo2.app.ContentPane">
		<properties>
			<property name="background" value="#284f32"/>
			<property name="backgroundImage">
				<fill-image repeat="no">
					<image type="nextapp.echo2.app.ResourceImageReference">
						<resource-image-reference resource="hse/theme/RedmondX/TitleBackground.jpg"/>
					</image>
				</fill-image>
			</property>
		</properties>
	</style>
